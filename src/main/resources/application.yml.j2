---
server:
  port: 9001
logging:
  level:
    org:
      apache:
        coyote:
          http11: DEBUG
spring:
  jackson:
    property-naming-strategy: SNAKE_CASE
    default-property-inclusion: non_null
registry_service:
  url: {{ registry_url }}
context:
  domain: LOCAL-RETAIL
  city: Bengaluru
  country: IND
  bap_id: qa.api.box.beckn.org
  bap_uri: http://qa.api.box.beckn.org/bap/v1
database:
  mongo:
    url: mongodb://{{ mongo_host }}:{{ mongo_port }}
    name: {{ bap_mongo_name }}
management:
  endpoints:
    web:
      exposure:
        include: "configprops, env, health, loggers, metrics, mappings, httptrace"
