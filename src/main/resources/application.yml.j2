---
server:
  port: 9001
spring:
  jackson:
    property-naming-strategy: SNAKE_CASE
    default-property-inclusion: non_null
registry_service:
  url: {{ registry_url }}
context:
  domain: local-retail
  city: Bengaluru
  country: IND
  bap_id: qa.api.box.beckn.org
  bap_uri: http://qa.api.box.beckn.org/v1
database:
  mongo:
    url: mongodb://{{ mongo_host }}:{{ mongo_port }}
    name: {{ bap_mongo_name }}
management:
  endpoints:
    web:
      exposure:
        include: "configprops, env, health, loggers, metrics, mappings"
